      "options": {
        "domain": "c2.example.com",
        "ttl": 60
      }
    },
    {
      "id": "ws1",
      "type": "websocket",
      "address": "0.0.0.0",
      "port": 8001,
      "options": {
        "path": "/ws"
      }
    },
    {
      "id": "icmp1",
      "type": "icmp",
      "address": "0.0.0.0",
      "port": 0,
      "options": {
        "protocol": "icmp"
      },
      "disabled": false
    }
  ]
