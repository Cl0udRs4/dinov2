{
  "listeners": [
    {
      "id": "tcp1",
      "type": "tcp",
      "address": "0.0.0.0",
      "port": 8080,
      "options": {}
    },
    {
      "id": "http1",
      "type": "http",
      "address": "0.0.0.0",
      "port": 8000,
      "options": {
        "use_http2": true,
        "enable_api": true
      }
    },
    {
